{
  "name": "creact",
  "version": "0.0.1",
  "description": "Minimalist React Implementation",
  "main": "dist/creact.js",
  "scripts": {
    "test": "npm run build && ava",
    "coverage": "npm run build && nyc ava",
    "report-coverage": "npm run coverage && nyc report --reporter=lcov | codecov",
    "build": "babel src --out-dir dist && babel examples --out-dir dist"
  },
  "ava": {
    "files": "tests/*.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/valmynd/creact.git"
  },
  "keywords": [
    "creact"
  ],
  "author": "C. Wilhelm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valmynd/creact/issues"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "codecov": "^1.0.1",
    "nyc": "^10.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "babel-runtime": "^6.20.0",
    "circular-json": "^0.3.1"
  }
}
