{
  "name": "creact",
  "version": "0.0.1",
  "description": "Minimalist React Implementation",
  "main": "dist/creact.js",
  "scripts": {
    "test": "npm run build && ava",
    "coverage": "npm run build-babel && nyc ava",
    "report-coverage": "npm run coverage && nyc report --reporter=lcov | codecov",
    "build-babel": "babel src --out-dir dist && babel examples --out-dir dist",
    "build": "webpack -p",
    "start": "webpack-dev-server --progress --colors --port 3000"
  },
  "ava": {
    "files": "tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/valmynd/creact.git"
  },
  "keywords": [
    "creact"
  ],
  "author": "C. Wilhelm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valmynd/creact/issues"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^2.2.0",
    "nyc": "^11.0.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "babel-runtime": "^6.20.0",
    "circular-json": "^0.3.1",
    "x3dom": "^1.7.2"
  }
}
