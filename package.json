{
  "name": "creact",
  "version": "0.0.1",
  "description": "Minimalist React Implementation",
  "main": "dist/creact.js",
  "scripts": {
    "test": "npm run build && ava",
    "coverage": "npm run build && nyc ava",
    "report-coverage": "npm run coverage && nyc report --reporter=lcov | codecov",
    "build": "babel src --out-dir dist"
  },
  "ava": {
    "files": "tests/*.js"
  },
  "babel": {
    "presets": [
      [
        "modern-browsers",
        {
          "loose": true
        }
      ]
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/valmynd/creact.git"
  },
  "keywords": [
    "creact"
  ],
  "author": "C. Wilhelm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valmynd/creact/issues"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-preset-modern-browsers": "^10.0.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "codecov": "^3.0.0",
    "nyc": "^11.4.1"
  },
  "dependencies": {}
}
